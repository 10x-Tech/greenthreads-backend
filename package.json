{
  "name": "pind-backend",
  "module": "index.ts",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "build": "echo 'Cleaning dist directory...' && rimraf dist && echo 'Compiling TypeScript...' && npx tsc && echo 'Build completed successfully!'",
    "prestart": "npm run build",
    "start": "node  dist/app.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\"  \"nodemon dist/app.js\"",
    "dev": "bun --watch src/app.ts",
    "seed": "node dist/seedData.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
